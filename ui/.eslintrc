{
    "parser": "babel-eslint",
    "env": {
        "browser": true,
        "node": true,
        "es6": true,
        "jest": true
    },
    "globals": {
        "React":true
    },
    "plugins": [
        "react"
    ],
  //  "extends": "airbnb",

    "rules": {
        "max-len": 0,
        "one-var": 0,
        "linebreak-style": 0, 
        "react/jsx-filename-extension": [1, { "extensions": [".ts", ".tsx"] }],
        "indent": "off"}}